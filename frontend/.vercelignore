# Vercel ignore file - exclude unnecessary files from deployment

# Backend files (not needed for frontend)
backend/
db/

# Development files
.git/
.github/

# IDE files
.vscode/
.idea/
*.swp
*.swo

# Environment files
.env.local
.env.development

# Test files
tests/
*.test.js
*.test.jsx
*.spec.js
*.spec.jsx

# Documentation
docs/
*.md
!README.md

# Node modules (installed by Vercel)
node_modules/

# Build artifacts (generated by Vercel)
dist/
.cache/

# Logs
*.log
logs/

# OS files
.DS_Store
Thumbs.db

# Misc
*.txt
devops/
